{
  "name": "resin-request",
  "version": "8.5.0",
  "description": "Resin.io HTTP client",
  "main": "build/request.js",
  "homepage": "https://github.com/resin-io-modules/resin-request",
  "repository": {
    "type": "git",
    "url": "git://github.com/resin-io-modules/resin-request.git"
  },
  "keywords": [
    "resin",
    "request",
    "http"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "test": "npm run test-node && npm run test-browser",
    "test-node": "gulp test",
    "test-browser": "karma start",
    "build": "rimraf ./build && npm run prettify && tsc",
    "prepublish": "npm run build && npm run readme && npm test",
    "readme": "jsdoc2md --template doc/README.hbs build/request.js build/progress.js build/utils.js > README.md",
    "prettify": "prettier --use-tabs --single-quote --write lib/*.ts"
  },
  "author": "Juan Cruz Viotti <juanchiviotti@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/bluebird": "3.5.6",
    "@types/bluebird-global": "^3.5.3",
    "@types/lodash": "^4.14.70",
    "@types/node": "^6.0.84",
    "@types/qs": "^6.5.0",
    "coffee-script": "~1.12.5",
    "global-tunnel-ng": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-coffee": "^2.3.1",
    "gulp-coffeelint": "^0.6.0",
    "gulp-mocha": "^4.3.0",
    "gulp-util": "^3.0.1",
    "jsdoc-to-markdown": "^3.0.0",
    "karma": "^1.3.0",
    "mocha": "^3.0.0",
    "mochainon": "^1.0.0",
    "prettier": "^1.5.2",
    "proxy": "^0.2.4",
    "resin-config-karma": "^1.0.4",
    "resin-fetch-mock": "^1.0.3",
    "resin-token": "^4.0.0",
    "rimraf": "^2.6.1",
    "temp": "^0.8.3",
    "timekeeper": "^1.0.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "fetch-ponyfill": "^4.0.0",
    "lodash": "^4.6.1",
    "progress-stream": "^2.0.0",
    "qs": "^6.3.0",
    "resin-errors": "^2.7.0",
    "rindle": "^1.3.1"
  },
  "peerDependencies": {
    "resin-token": "^4.0.0"
  }
}
