{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "outDir": "build",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "pretty": true,
    "removeComments": true,
    "sourceMap": true
  },
  "include": [
		"lib/**/*.d.ts",
    "lib/**/*.ts"
  ]
}
